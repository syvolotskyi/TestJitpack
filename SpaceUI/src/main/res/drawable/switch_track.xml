<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="false" android:state_checked="false"
        android:drawable="@drawable/switch_track_disabled_unchecked" />
    <item android:state_enabled="false" android:state_checked="true"
        android:drawable="@drawable/switch_track_disabled_checked" />
    <item android:state_enabled="true" android:state_checked="false"
        android:drawable="@drawable/switch_track_enabled_unchecked" />
    <item android:state_enabled="true" android:state_checked="true"
        android:drawable="@drawable/switch_track_enabled_checked" />
    <item android:drawable="@drawable/switch_track_enabled_unchecked" />
</selector>